# Create vectors
names <- c("siri", "mahi", "chiru")
age <- c(23, 24, 25)
marks <- c(88, 78, 25)

# Create data frame
df <- data.frame(names, age, marks)

# Calculate mid-range of age
mid_range_age <- (min(df$age) + max(df$age)) / 2
mid_range_age

# Export to CSV
write.csv(df, "datafr.csv", row.names = FALSE)
