# Sample numeric vector
values <- c(23, 24, 25, 30, 35, 40)

# Minimum
min_val <- min(values)

# Maximum
max_val <- max(values)

# Mean
mean_val <- mean(values)

# Min-Max (Range)
minmax_val <- max_val - min_val

# Print results
cat("Minimum:", min_val, "\n")
cat("Maximum:", max_val, "\n")
cat("Mean:", mean_val, "\n")
cat("Min-Max (Range):", minmax_val, "\n")
